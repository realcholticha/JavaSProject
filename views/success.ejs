                        <h4><%= room.date %> </h4>
                        <h4><%= room.Time[0] %> | <%= room.Time[1] %></h4>
                    </div> 
                        
                </div>
               
            </div>
        <% }) %>
        <div class="container content mt"> 
                <p><a href="/checkroom" class="box red-box">ยืนยัน</a></p>                    
